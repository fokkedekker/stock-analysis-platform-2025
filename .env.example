# Financial Modeling Prep API Key (required)
FMP_API_KEY=your_api_key_here

# Database path (optional, defaults to data/stock_analysis.duckdb)
DATABASE_PATH=data/stock_analysis.duckdb

# Rate limit per minute (optional, defaults to 700 for FMP Premium)
RATE_LIMIT_PER_MINUTE=700

# Number of parallel workers for data fetching (optional, defaults to 10)
NUM_WORKERS=10

# Cerebras API Key for AI stock explanations (optional)
CEREBRAS_API_KEY=your_cerebras_api_key_here
